{
  "dashboard": {
    "title": "Neural DSL Model Monitoring",
    "tags": ["neural", "ml", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Predictions Per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(neural_predictions_total[5m])",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
      },
      {
        "id": 2,
        "title": "Prediction Latency (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(neural_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
      },
      {
        "id": 3,
        "title": "Model Accuracy",
        "type": "graph",
        "targets": [
          {
            "expr": "neural_model_accuracy",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
      },
      {
        "id": 4,
        "title": "Drift Score",
        "type": "graph",
        "targets": [
          {
            "expr": "neural_drift_score",
            "legendFormat": "{{drift_type}}"
          }
        ],
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8}
      },
      {
        "id": 5,
        "title": "Data Quality Score",
        "type": "graph",
        "targets": [
          {
            "expr": "neural_data_quality_score",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8}
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(neural_errors_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8}
      },
      {
        "id": 7,
        "title": "Missing Values Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "neural_missing_values_rate",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 0, "y": 24, "w": 8, "h": 8}
      },
      {
        "id": 8,
        "title": "Outlier Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "neural_outlier_rate",
            "legendFormat": "{{model}} v{{version}}"
          }
        ],
        "gridPos": {"x": 8, "y": 24, "w": 8, "h": 8}
      },
      {
        "id": 9,
        "title": "Active Models",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_active_models"
          }
        ],
        "gridPos": {"x": 16, "y": 24, "w": 8, "h": 8}
      }
    ],
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 1
  }
}
